\mode<presentation>

% \definecolor{MSUgreen}{RGB}{0,102,51} 
\definecolor{SBgreen}{RGB}{160,155,89} 
% \definecolor{SByellow}{RGB}{215,169,0} 
% Approximate match for Michigan State University; official colour appears too dark

\setbeamercolor{alerted text}{fg=SBgreen!80!white}
\setbeamercolor*{palette primary}{fg=SBgreen!60!black,bg=white!85!SBgreen}
\setbeamercolor*{palette secondary}{fg=SBgreen!70!black,bg=white!60!SBgreen}
\setbeamercolor*{palette tertiary}{bg=SBgreen!80!black,fg=white!50!SBgreen}
\setbeamercolor*{palette quaternary}{fg=SBgreen,bg=white!20!SBgreen}

\setbeamercolor*{sidebar}{fg=SBgreen,bg=SBgreen!75!white}

\setbeamercolor*{palette sidebar primary}{fg=SBgreen!10!black}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{fg=SBgreen!50!black}
\setbeamercolor*{palette sidebar quaternary}{fg=white!10!SBgreen}

\setbeamercolor*{titlelike}{parent=palette primary}
\setbeamercolor{frametitle}{bg=white!90!SBgreen}
\setbeamercolor{frametitle right}{bg=white!60!SBgreen}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}
\setbeamercolor{block body}{parent=normal text,use=block title,bg=red!0,fg=}
\mode
<all>
